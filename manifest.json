manifest_version”: 2,
 “name”: “CrawlNSpellCheck”,
 “description”: “Crawls the webpage upto 2 levels, checks spellings and suggests corrected spellings”,
 “version”: “1”,
 “author”: “Colin Dsouza”,
"browser_action": {
   "default_icon": "tab-icon.png",
   “default_title”: “Have a good day”
 },
“chrome_url_overrides” : {
  “newtab”: “newtab.html”
},
 “permissions”: [“activeTab”]
}
